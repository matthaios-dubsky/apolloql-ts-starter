{
  "author": "Dubs",
  "name": "my-ts-starter",
  "version": "1.0.0",
  "description": "a simple starter for typescript with my own setup",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npm run lint && npm test && npm run webpack:prod",
    "build:gcp": "npm run lint && npm test && tsc -p . --outDir ./gcp && cp package* gcp/",
    "watch": "tsc -w -p .",
    "webpack:dev": "webpack --mode=development",
    "webpack:prod": "webpack --mode=production",
    "lint": "eslint --fix **/*.ts",
    "test": "jest --coverage --verbose",
    "clean": "rm -rf dist/* && rm -rf gcp/*"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.39",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "0.0.4",
    "jest": "^26.0.1",
    "nock": "^10.0.6",
    "npm-audit-resolver": "^2.2.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@types/ramda": "^0.26.44",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "apollo-boost": "^0.4.9",
    "apollo-server": "^2.13.1",
    "apollo-server-express": "^2.13.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "ramda": "^0.26.1",
    "type-graphql": "^0.17.6",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "winston": "^3.2.1"
  }
}
